@model IEnumerable<Info>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

@foreach (var i in Model)
{
    <div class="form-group">
        <fieldset disabled="">
            <label class="form-label">Name</label>
            <input class="form-control" type="text" value="@i.Name" disabled="">
        </fieldset>
    </div>
    <div class="form-group">
        <img src="@i.GetSrc" style="width:200px" class="img-thumbnail" />
    </div>
}